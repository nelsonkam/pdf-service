{
  "name": "pdf-service",
  "version": "1.0.0",
  "description": "A PDF micro service",
  "main": "./dist/index.js",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "start:worker": "npm run build && cross-env NODE_ENV=production node dist/worker.js",
    "dev": "cross-env NODE_ENV=development nodemon --config nodemon-server.json",
    "dev:worker": "cross-env NODE_ENV=development nodemon --config nodemon-worker.json",
    "docker:dev:up": "docker compose -f docker-compose.dev.yml --env-file .env up --build",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml --env-file .env down",
    "docker:dev:sh": "docker compose -f docker-compose.dev.yml --env-file .env run app bash",
    "docker:test:up": "docker compose -p pdf-service-test -f docker-compose.test.yml --env-file .env up --build --abort-on-container-exit",
    "docker:test:down": "docker compose -p pdf-service-test -f docker-compose.test.yml --env-file .env down",
    "test:integration": "jest --forceExit src/tests/integration",
    "build": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@bull-board/api": "^4.2.2",
    "@bull-board/express": "^4.2.2",
    "@prisma/client": "^4.3.1",
    "axios": "^0.27.2",
    "bullmq": "^1.90.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "class-validator-jsonschema": "^3.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "gm": "^1.23.1",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "ioredis": "^5.2.3",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.1",
    "routing-controllers-openapi": "^2.2.0",
    "swagger-ui-express": "^4.5.0",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/gm": "^1.18.12",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^28.1.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-mock-extended": "^2.0.7",
    "lint-staged": "^13.0.3",
    "nock": "^13.2.9",
    "node-config": "^0.0.2",
    "node-gyp": "^9.1.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.8.3"
  }
}
